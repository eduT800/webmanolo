u{\r\n\tdisplay: grid;\r\n\tmargin: auto;\r\n\tjustify-content: center;\r\n\talign-items: center;\r\n\ttext-align: center;\r\n\tmargin-top: 16px;\r\n\tmax-width: 600px;\r\n}\r\n\r\n.menuIn{\r\n\tdisplay: flex;\r\n\tjustify-content: center;\r\n\talign-items: center;\r\n\tmin-width: 290px;\r\n\theight: 48px;\r\n\tmargin-top: 10px;\r\n\tmargin-bottom: 2px;\r\n\tborder: 2px solid /*#730000*/silver;\r\n\tbackground: #300000;\r\n\tcolor: #ffe251;\r\n\tfont-weight: bolder;\r\n\tborder-radius: 12px;\r\n\tbox-shadow: 0px 0px 12px #300000;\r\n\ttransition: .3s;\r\n}\r\n\r\n.menuIn a{\r\n\ttext-decoration: none;\r\n\tcolor: #ffe251;\r\n\tpadding: 10px 40px;\r\n}\r\n\r\n.menuIn:hover{\r\n\ttransform: scale(1.1);\r\n\tbackground: coral;\r\n\tcolor: #300000;\r\n}\r\n\r\n.menuIn a:hover{\r\n\tcolor: #300000;\r\n}\r\n\r\n#tapas, #ensaladas, #bocadillos, #bocadillosEspeciales, #hamburguesa, #combinados, #especiales{\r\n\tdisplay: flex;\r\n\tflex-direction: column;\r\n\tmax-width: 400px;\r\n\tmargin: auto;\r\n\tmargin-top: 25px;\r\n}\r\n\r\n.grid{\r\n\tdisplay: grid;\r\n\tgrid-template-columns: 28px 2fr 1fr 1fr;\r\n\tmargin-top: 15px;\r\n\talign-items: center;\r\n\tjustify-content: center;\r\n\t/*grid-gap: 6px;*/\r\n\t/*height: 22px;*/\r\n\t/*padding-top: 6px;\r\n\tpadding-bottom: 6px;*/\r\n\theight: auto;\r\n}\r\n/*\r\n.nombre{\r\n\tpadding-top: 6px;\r\n\tpadding-bottom: 6px;\r\n}\r\n*/\r\n.precio{\r\n\ttext-align: right;\r\n\tpadding-right: 8px;\r\n}\r\n.alergenos img{\r\n\twidth: 25px;\r\n\t/*padding-left: 8px;*/\r\n}\r\n\r\n.patatas{\r\n\tmargin-top: 24px;\r\n\tfont-style: italic;\r\n}\r\n\r\n.guiaAlergenos{\r\n\ttext-align: center;\r\n\tmargin-top: 25px;\r\n}\r\n\r\n.guiaAlergenos img{\r\n\tmax-width: 80px;\r\n}\r\n.num{\r\n\tpadding-left: 6px;\r\n}\r\n.nombre{\r\n\tpadding-left: 6px;\r\n}"},"lineCount":195}},"error":null,"hash":"3085a73e57b730d567ee2f51e53fdf53","cacheData":{"env":{}}}